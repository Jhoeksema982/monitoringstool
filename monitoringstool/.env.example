# Copy this file to .env and fill in values before deploying

# Supabase
SUPABASE_URL=
SUPABASE_KEY=

# Server
PORT=5000
NODE_ENV=production
CORS_ORIGIN=
ADMIN_EMAILS=you@example.com

# Rate limiting (optional)
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Optional: allow server to serve static frontend when NODE_ENV=production
SERVE_STATIC=true
